    "html2canvas": "^1.4.1",
    "mapbox-gl": "^3.0.0",
    "object-hash": "^3.0.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-file-selector": "^2.0.2",
    "vue-i18n": "^9.8.0",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.4",
    "vuex": "^4.0.2"
